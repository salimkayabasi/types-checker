{
  "name": "types-checker",
  "version": "0.0.10",
  "description": "Checker for Typescript definitions for modules",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salimkayabasi/types-checker.git"
  },
  "bugs": {
    "url": "https://github.com/salimkayabasi/types-checker/issues"
  },
  "homepage": "https://github.com/salimkayabasi/types-checker#readme",
  "author": "Salim KAYABAÅžI <salim.kayabasi@gmail.com>",
  "license": "MIT",
  "bin": {
    "types-checker": "./bin/types-checker.js",
    "ts-checker": "./bin/types-checker.js",
    "tscheck": "./bin/types-checker.js"
  },
  "main": "src/types-checker.js",
  "scripts": {
    "major": "npm version major -m \"New major version %s\"",
    "minor": "npm version minor -m \"New minor version %s\"",
    "patch": "npm version patch -m \"New patch version %s\"",
    "lint": "eslint --ext js,json,md .",
    "lint:fix": "npm run lint --- --fix",
    "test": "NODE_ENV=test jest --forceExit --ci --errorOnDeprecated --detectOpenHandles --runInBand --passWithNoTests",
    "test:watch": "NODE_ENV=test jest --watch",
    "prepare": "npm run lint && npm run test"
  },
  "dependencies": {
    "async": "^3.1.0",
    "chalk": "^3.0.0",
    "commander": "^4.1.0",
    "inquirer": "^7.0.3",
    "lodash": "^4.17.15",
    "log4js": "^6.1.0",
    "ora": "^4.0.3"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.3.0",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-markdown": "^1.0.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0"
  },
  "jest": {
    "testMatch": [
      "**/(__tests__|tests|test)/**/*.test.js?(x)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/build/**",
      "!**/dist/**",
      "!**/bin/**",
      "!**/config/**",
      "!**/test/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
